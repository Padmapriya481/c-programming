#include<stdio.h>
int main()
{
    int ch;
    printf("enter the choice:");
    scanf("%d",&ch);
    switch(ch){
        case 1:
        printf("one");
        break;
        case 2:
        printf("two");
        break;
        default:
        printf("invalid");
        break;
    }
    return 0;
}
    
