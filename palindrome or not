#include <stdio.h>

int main() {
    int temp,n,s=0,r;
    printf("enter the number:");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        r=n%10;
        s=(s*10)+r;
        n=n/10;
        
    }
    if(temp==s)
    {
        printf("palindrome");
    }
    else{
        printf("not a palindrome");
    }
    return 0;
}
